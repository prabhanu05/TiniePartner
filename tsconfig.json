{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "suppressImplicitAnyIndexErrors": true,
        "baseUrl": ".",
        "paths": {
            "@screens/*": ["screens/*"],
            "@styles/*": ["styles/*"],
            "@constants/*": ["constants/*"],
            "@routes/*": ["routes/*"],
            "@models/*": ["models/*"],
            "@common/*": ["components/common/*"],
            "@components/*": ["components/pages/*"],
            "@api/*": ["api/*"],
            "@store/*": ["store/*"],
            "@svg/*": ["svg/*"],
            "@fonts/*": ["fonts/*"],
            "@hooks/*": ["hooks/*"]
        }
    }
}
